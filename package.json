{
  "name": "lit-course",
  "version": "1.0.0",
  "description": "Lit course",
  "main": "index.js",
  "scripts": {
    "test": "karma start --coverage",
    "test:watch": "karma start --browsers=Chrome --single-run=false --auto-watch",
    "test:mock": "mockserver -p 8080 -m tests/mock",
    "test:e2e": "mocha --recursive --timeout 10000 ./e2e/tests/*.js",
    "test:google": "mocha --recursive --timeout 10000 ./e2e/tests/google-example/*.js",
    "dev:background": "nohup npm run dev &",
    "dev:kill": "kill -9 $(lsof -t -i:2900 -sTCP:LISTEN)",
    "test:e2e-ci": "headless=true npm run dev:background && ./scripts/utils/wait-launch.sh && npm run test:e2e && npm run dev:kill",
    "dev": "node ./scripts/dev.js",
    "build": "node ./scripts/build.js",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint --fix ./src/**"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@open-wc/testing": "^4.0.0",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-json": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.3.0",
    "mocha": "^5.2.0",
    "mockserver": "^3.1.1",
    "prettier": "^2.1.2",
    "pwa-helpers": "^0.9.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-delete": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-serve": "^1.0.1",
    "sinon": "^9.2.0",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@seed-catalog/styles": "^1.0.0",
    "@vaadin/router": "^1.2.0",
    "ip": "^1.1.5",
    "lit-element": "^2.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rollup-plugin-babel": "^4.3.2"
  },
  "overrides": {
    "rollup": "^3.0.0-8"
  }
}
